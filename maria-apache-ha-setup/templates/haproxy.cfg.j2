global
        log 127.0.0.1   local0
        log 127.0.0.1   local1 debug
        maxconn   45000 
        daemon
        nbproc      2
defaults
        timeout server 86400000
        timeout connect 86400000
        timeout client 86400000
        timeout queue   1000s

# [HTTP Site Configuration]
listen  http_web 192.199.0.205:80
        mode http
        balance roundrobin  
        option httpchk
        option forwardfor
        server app1 app1.b576.internal:80 weight 1 maxconn 512 check
        server app2 app2.b576.internal:80 weight 1 maxconn 512 check
